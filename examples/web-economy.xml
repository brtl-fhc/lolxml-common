<?xml version="1.0" encoding="UTF-8"?>
<grammar xmlns="http://lolxml.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://lolxml.org http://lolxml.org ">
	
    <!-- Contains excerpts from: http://www.dack.com/web/bullshit.html -->

	<data />

	<sym id="VERB"><switch>
	    <case>aggregate</case>
		<case>benchmark</case>
		<case>cultivate</case>
		<case>deploy</case>
		<case>drive</case>
		<case>embrace</case>
		<case>enable</case>
		<case>engineer</case>
		<case>envisioneer</case>
		<case>expedite</case>
		<case>extend</case>
		<case>generate</case>
		<case>harness</case>
		<case>incentivize</case>
		<case>innovate</case>
		<case>iterate</case>
		<case>leverage</case>
		<case>maximize</case>
		<case>monetize</case>
		<case>optimize</case>
		<case>productize</case>
		<case>redefine</case>
		<case>reinvent</case>
		<case>revolutionize</case>
		<case>seize</case>
		<case>streamline</case>
		<case>synergize</case>
		<case>target</case>
		<case>transition</case>
		<case>utilize</case>
		<case>whiteboard</case></switch></sym>
	
	<sym id="ADJ"><switch>
	    <case>24/7</case>
		<case>B2C</case>
		<case>best-of-breed</case>
		<case>bricks-and-clicks</case>
		<case>collaborative</case>
		<case>cross-platform</case>
		<case>customized</case>
		<case>distributed</case>
		<case>dynamic</case>
		<case>efficient</case>
		<case>enterprise</case>
		<case>frictionless</case>
		<case>global</case>
		<case>holistic</case>
		<case>innovative</case>
		<case>interactive</case>
		<case>killer</case>
		<case>magnetic</case>
		<case>next-generation</case>
		<case>open-source</case>
		<case>plug-and-play</case>
		<case>real-time</case>
		<case>rich</case>
		<case>scalable</case>
		<case>sexy</case>
		<case>strategic</case>
		<case>transparent</case>
		<case>ubiquitous</case>
		<case>value-added</case>
		<case>viral</case>
