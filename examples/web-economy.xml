	<sym id="NOUN"><switch>
	    <case>applications</case>
		<case>bandwidth</case>
		<case>communities</case>
		<case>convergence</case>
		<case>e-business</case>
		<case>e-markets</case>
		<case>e-tailers</case>
		<case>eyeballs</case>
		<case>infomediaries</case>
		<case>initiatives</case>
		<case>markets</case>
		<case>metrics</case>
		<case>models</case>
		<case>niches</case>
		<case>partnerships</case>
		<case>portals</case>
		<case>ROI</case>
		<case>web-readiness</case>
		<case>solutions</case>
		<case>systems</case>
		<case>users</case>
		<case>web services</case></switch></sym>
    
	
	<sym id="ROOT">
	    <eval idref="VERB"/> <eval idref="ADJ" /> <eval idref="NOUN" />
	</sym>

	<eval idref="ROOT"/>

</grammar>
