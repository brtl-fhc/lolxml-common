<?xml version="1.0" encoding="UTF-8"?>
<lol:grammar xmlns:lol="http://lolxml.org" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://lolxml.org ../../main/resources/lolxml.xsd ">
  <lol:data>
  	<name>World</name>
  </lol:data>
  <lol:sym id="ROOT">
    <html xmlns="http://www.w3.org/1999/xhtml"><body><p class="cl1">Hello <b><lol:exp value="name"/>!</b></p></body></html>
  </lol:sym>
  <lol:eval idref="ROOT"/>
</lol:grammar>
